  "version": "0.2.0",
  "configurations": [
    {
      "name": "linux-nvhpc-multicore-debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/linux-nvhpc-multicore-debug/monaco",
      "args": [
        "50000"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "/usr/bin/gdb"
